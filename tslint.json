{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
	"quotemark": [true, "single", "avoid-escape", "avoid-template"],
	"indent": [true, "spaces", 2],
	"no-console": false,
	"eofline": false,
	"trailing-comma": false,
	"object-literal-sort-keys": false
    },
    "rules": {},
    "rulesDirectory": []
}